{
  "name": "@netlify/remix-edge-adapter",
  "version": "3.1.0",
  "description": "Remix Adapter for Netlify Edge Functions",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prepack": "npm run clean && npm run build",
    "build": "tshy",
    "build:watch": "npm run build -- --watch",
    "clean": "rm -rf build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/netlify/remix-compute.git"
  },
  "keywords": [
    "remix",
    "netlify"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/netlify/remix-compute/issues"
  },
  "homepage": "https://github.com/netlify/remix-compute#readme",
  "dependencies": {
    "@netlify/remix-runtime": "2.1.0",
    "isbot": "^3.7.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.0.0",
    "tshy": "^1.7.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "tshy": {
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/index.ts"
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    }
  },
  "main": "./dist/commonjs/index.js",
  "types": "./dist/commonjs/index.d.ts",
  "type": "module"
}
