{
  "name": "@netlify/remix-edge-adapter",
  "version": "3.2.0",
  "description": "Remix Adapter for Netlify Edge Functions",
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./plugin": {
      "require": {
        "types": "./dist/plugin.d.ts",
        "default": "./dist/plugin.js"
      },
      "import": {
        "types": "./dist/plugin.d.mts",
        "default": "./dist/plugin.mjs"
      }
    },
    "./entry.server": "./src/entry.server.tsx"
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md",
    "plugin.js",
    "plugin.d.ts",
    "src/entry.server.tsx",
    "src/server.ts"
  ],
  "scripts": {
    "prepack": "pnpm run build",
    "build": "tsup-node src/index.ts src/plugin.ts --format esm,cjs --dts --target node16 --clean",
    "build:watch": "pnpm run build --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/netlify/remix-compute.git",
    "directory": "packages/remix-edge-adapter"
  },
  "keywords": [
    "remix",
    "netlify"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/netlify/remix-compute/issues"
  },
  "homepage": "https://github.com/netlify/remix-compute#readme",
  "dependencies": {
    "@netlify/remix-runtime": "2.2.0",
    "isbot": "^4.1.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.2",
    "vite": "^5.1.3"
  },
  "peerDependencies": {
    "vite": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}