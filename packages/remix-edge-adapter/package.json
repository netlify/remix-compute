{
  "name": "@netlify/remix-edge-adapter",
  "version": "3.1.0",
  "description": "Remix Adapter for Netlify Edge Functions",
  "types": "./build/src/index.d.ts",
  "main": "./build/src/index.js",
  "exports": {
    ".": "./build/src/index.js"
  },
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.js.map",
    "build/src/**/*.d.ts",
    "build/src/**/*.d.ts.map",
    "resources/**/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prepack": "pnpm run clean && pnpm run build",
    "build": "tsc --build tsconfig.json",
    "build:watch": "pnpm run build -- --watch",
    "clean": "rm -rf build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/netlify/remix-compute.git",
    "directory": "packages/remix-edge-adapter"
  },
  "keywords": [
    "remix",
    "netlify"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/netlify/remix-compute/issues"
  },
  "homepage": "https://github.com/netlify/remix-compute#readme",
  "dependencies": {
    "@netlify/remix-runtime": "workspace:*",
    "isbot": "^3.7.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.6.0",
    "@remix-run/react": "^2.6.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^5.1.3"
  },
  "peerDependencies": {
    "vite": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  }
}