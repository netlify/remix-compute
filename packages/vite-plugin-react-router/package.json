{
  "name": "@netlify/vite-plugin-react-router",
  "version": "2.0.1",
  "description": "React Router 7+ Vite plugin for Netlify",
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./serverless": {
      "types": "./dist/serverless.d.mts",
      "default": "./dist/serverless.mjs"
    },
    "./edge": {
      "types": "./dist/edge.d.mts",
      "default": "./dist/edge.mjs"
    }
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "prepack": "pnpm run build",
    "build": "rm -rf dist && tsup-node",
    "build:watch": "pnpm run build --watch",
    "publint": "publint --strict"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/netlify/remix-compute",
    "directory": "packages/vite-plugin-react-router"
  },
  "keywords": [
    "react-router",
    "vite-plugin",
    "netlify"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/netlify/remix-compute/issues"
  },
  "homepage": "https://github.com/netlify/remix-compute#readme",
  "dependencies": {
    "isbot": "^5.0.0"
  },
  "devDependencies": {
    "@netlify/edge-functions": "^3.0.1",
    "@netlify/functions": "^3.1.10",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^7.9.4",
    "tsup": "^8.0.2",
    "vite": "^6.2.5"
  },
  "peerDependencies": {
    "react-router": ">=7.9.0",
    "vite": ">=5.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  }
}
