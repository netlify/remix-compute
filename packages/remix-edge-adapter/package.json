{
  "name": "@netlify/remix-edge-adapter",
  "version": "4.0.0",
  "description": "Remix Adapter for Netlify Edge Functions",
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./plugin": {
      "require": {
        "types": "./dist/plugin.d.ts",
        "default": "./dist/plugin.js"
      },
      "import": {
        "types": "./dist/plugin.d.mts",
        "default": "./dist/plugin.mjs"
      }
    },
    "./entry.server": "./src/entry.server.tsx"
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md",
    "plugin.js",
    "plugin.d.ts",
    "src/entry.server.tsx",
    "src/server.ts"
  ],
  "scripts": {
    "prepack": "pnpm run build",
    "build": "pnpm run build:src && pnpm run build:types",
    "build:src": "tsup-node src/index.ts src/plugin.ts --format esm,cjs --dts --target node20 --clean",
    "build:types": "deno types > deno.d.ts",
    "build:watch": "pnpm run build:src --watch",
    "publint": "publint --strict"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/netlify/remix-compute.git",
    "directory": "packages/remix-edge-adapter"
  },
  "keywords": [
    "remix",
    "netlify"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/netlify/remix-compute/issues"
  },
  "homepage": "https://github.com/netlify/remix-compute#readme",
  "dependencies": {
    "@netlify/remix-runtime": "3.0.0",
    "@remix-run/dev": "^2.16.4",
    "@remix-run/node": "^2.16.4",
    "isbot": "^5.0.0"
  },
  "devDependencies": {
    "@remix-run/react": "^2.16.4",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.0.2",
    "vite": "^6.2.5"
  },
  "peerDependencies": {
    "vite": ">=5.0.0"
  },
  "engines": {
    "node": ">=20.6.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
